./jre/bin/java -cp plugins/org.eclipse.ui.test.harness.launcher/bin;startup.jar org.eclipse.ui.test.harness.launcher.TestUIMain -dev bin -data ui_session_sniff_folder -uiTest ui.api.SessionRestoreTest
