<?xml version="1.0" encoding="UTF-8"?>

<plugin
   name="%Plugin.name"
   id="org.eclipse.ui"
   version="2.0"
   provider-name="Object Technology International, Inc."
   class="org.eclipse.ui.internal.WorkbenchPlugin">
   
   <requires>
      <import plugin="org.apache.xerces"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.swt" export="true" /> 
   </requires>

<runtime>
  <library name="workbench.jar" >
     <export name = "*"/>
  </library> 
</runtime>

<extension id="workbench" point="org.eclipse.core.runtime.applications">
  <application> 
     <run class="org.eclipse.ui.internal.Workbench">
	<parameter name="productInfo" value="product.ini"/>
     </run>
  </application>
</extension>

<!-- Workbench extension point definitions -->

<extension-point name="%ExtPoint.acceleratorSets" id="acceleratorSets" />
<extension-point name="%ExtPoint.acceleratorConfigurations" id="acceleratorConfigurations" />
<extension-point name="%ExtPoint.acceleratorScopes" id="acceleratorScopes" />
<extension-point name="%ExtPoint.actionDefinitions" id="actionDefinitions" />
<extension-point name="%ExtPoint.perspectives" id="perspectives"/>
<extension-point name="%ExtPoint.perspectiveExtensions" id="perspectiveExtensions"/>
<extension-point name="%ExtPoint.preferencePages" id="preferencePages"/>
<extension-point name="%ExtPoint.propertyPages" id="propertyPages"/>
<extension-point name="%ExtPoint.editors" id="editors"/>
<extension-point name="%ExtPoint.views" id="views"/>
<extension-point name="%ExtPoint.actionSets" id="actionSets"/>
<extension-point name="%ExtPoint.viewMenus" id="viewActions"/>
<extension-point name="%ExtPoint.editorMenus " id="editorActions"/>
<extension-point name="%ExtPoint.popupMenus " id="popupMenus"/>
<extension-point name="%ExtPoint.importWizards" id="importWizards"/>
<extension-point name="%ExtPoint.exportWizards" id="exportWizards"/>
<extension-point name="%ExtPoint.newWizards" id="newWizards"/>
<extension-point name="%ExtPoint.elementFactories" id="elementFactories"/>
<extension-point name="%ExtPoint.dropActions" id="dropActions"/>
<extension-point name="%ExtPoint.resourceFilters" id="resourceFilters"/>
<extension-point name="%ExtPoint.markerImageProviders" id="markerImageProviders"/>
<extension-point name="%ExtPoint.projectNatureImages" id="projectNatureImages"/>
<extension-point name="%ExtPoint.decorators" id="decorators"/>
<extension-point name="%ExtPoint.markerHelp" id="markerHelp"/>
<extension-point name="%ExtPoint.markerResolution" id="markerResolution"/>
<extension-point name="%ExtPoint.capabilities" id="capabilities"/>

<!-- Extensions -->

<extension point = "org.eclipse.ui.actionDefinitions">
	<actionDefinition
		id = "file.close"/>
	<actionDefinition
		id = "file.closeAll"/>		
    <actionDefinition 
        id = "file.save"/>
    <actionDefinition 
        id = "file.saveAll"/>
    <actionDefinition 
        id = "file.print"/>
    <actionDefinition 
        id = "edit.undo"/>
    <actionDefinition 
        id = "edit.redo"/>
    <actionDefinition 
        id = "edit.cut"/>
    <actionDefinition 
        id = "edit.copy"/>
    <actionDefinition 
        id = "edit.paste"/>
    <actionDefinition 
        id = "edit.delete"/>        
    <actionDefinition 
        id = "edit.selectAll"/>
    <actionDefinition 
        id = "edit.findReplace"/>
    <actionDefinition
    	id = "edit.addBookmark"/>
    <actionDefinition
    	id = "window.switchToEditor"/>
    <actionDefinition
    	id = "window.showSystemMenu"/>
    <actionDefinition
    	id = "window.showViewMenu"/>
    <actionDefinition 
        id = "window.activeEditor"/>
    <actionDefinition 
        id = "window.nextEditor"/>
    <actionDefinition 
        id = "window.previousEditor"/>
    <actionDefinition 
        id = "window.nextView"/>
    <actionDefinition 
        id = "window.previousView"/>
</extension>

<extension point = "org.eclipse.ui.acceleratorConfigurations"> 
    <acceleratorConfiguration 
        id = "org.eclipse.ui.defaultAcceleratorConfiguration" 
        name = "Default" 
        description = "Eclipse's default accelerator configuration"/> 
    <acceleratorConfiguration 
        id = "org.eclipse.ui.emacsAcceleratorConfiguration" 
        name = "Emacs" 
        description = "Emacs editor style accelerator configuration"/> 
</extension> 
<extension point = "org.eclipse.ui.acceleratorScopes"> 
    <acceleratorScope 
        id = "org.eclipse.ui.globalScope" 
        name = "Global" 
        description = "Action accelerator key applicable to all views and editors unless explicitly overridden."/> 
    <acceleratorScope 
        id = "org.eclipse.ui.textEditorScope" 
        name = "Text Editor" 
        description = "Action accelerator key applicable only when text editor active." 
        parentScope = "org.eclipse.ui.globalScope" /> 
</extension> 

<extension point = "org.eclipse.ui.acceleratorSets">
	<acceleratorSet 
    	configurationId = "org.eclipse.ui.emacsAcceleratorConfiguration" 
    	scopeId = "org.eclipse.ui.globalScope"> 
        <accelerator
        	id = "file.close"
        	key = "Ctrl+X Ctrl+K" />
        <accelerator
        	id = "file.closeAll"
        	key = "Ctrl+Shift+F4||Ctrl+X Ctrl+C" />
        <accelerator
        	id = "file.save"
        	key = "Ctrl+X Ctrl+S" />
        <accelerator
        	id = "file.saveAll"
        	key = "" />
        <accelerator
        	id = "file.print"
        	key = "Alt+F9" />
        <accelerator
        	id = "edit.undo"
        	key = "F9" />
        <accelerator
        	id = "edit.redo"
        	key = "F10" />
        <accelerator
        	id = "edit.cut"
        	key = "Ctrl+W||Shift+DELETE" />
        <accelerator
        	id = "edit.copy"
        	key = "Alt+W||Ctrl+INSERT" />
        <accelerator
        	id = "edit.paste"
        	key = "Ctrl+Y||Shift+INSERT" />
        <accelerator
        	id = "edit.delete"
        	key = "" />
        <accelerator
        	id = "edit.selectAll"
        	key = "" />
        <accelerator
        	id = "edit.findReplace"
        	key = "Ctrl+S" />
        <accelerator
        	id = "edit.search"
        	key = "Ctrl+H" />
        <accelerator
        	id = "window.switchToEditor"
        	key = "Ctrl+X Ctrl+B" />
        <accelerator
        	id = "window.showSystemMenu"
        	key = "Alt+-" />
        <accelerator
        	id = "window.showViewMenu"
        	key = "Ctrl+F10" />
        <accelerator
        	id = "window.activeEditor"
        	key = "F12" />
        <accelerator
        	id = "window.nextEditor"
        	key = "Ctrl+F6" />
        <accelerator
        	id = "window.previousEditor"
        	key = "Ctrl+Shift+F6" />
        <accelerator
        	id = "window.nextView"
        	key = "Ctrl+F7" />
        <accelerator
        	id = "window.previousView"
        	key = "Ctrl+Shift+F7" />
    </acceleratorSet>
    <acceleratorSet
    	configurationId = "org.eclipse.ui.defaultAcceleratorConfiguration"
    	scopeId = "org.eclipse.ui.globalScope">
        <accelerator
        	id = "file.close"
        	key = "Ctrl+F4" />
        <accelerator
        	id = "file.closeAll"
        	key = "Ctrl+Shift+F4" />
        <accelerator
        	id = "file.save"
        	key = "Ctrl+S" />
        <accelerator
        	id = "file.saveAll"
        	key = "Ctrl+Shift+S" />
        <accelerator
        	id = "file.print"
        	key = "Ctrl+P" />
        <accelerator
        	id = "edit.undo"
        	key = "Ctrl+Z"/>
        <accelerator
        	id = "edit.redo"
        	key = "Ctrl+Y" />
        <accelerator
        	id = "edit.cut"
        	key = "Ctrl+X" />
        <accelerator
        	id = "edit.copy"
        	key = "Ctrl+C" />
        <accelerator
        	id = "edit.paste"
        	key = "Ctrl+V" />
        <accelerator
        	id = "edit.delete"
        	key = "Ctrl+DELETE" />
        <accelerator
        	id = "edit.selectAll"
        	key = "Ctrl+A" />
        <accelerator
        	id = "edit.findReplace"
        	key = "Ctrl+F" />
        <accelerator
        	id = "window.switchToEditor"
        	key = "Ctrl+W" />
        <accelerator
        	id = "window.showSystemMenu"
        	key = "Alt+-" />
        <accelerator
        	id = "window.showViewMenu"
        	key = "Ctrl+F10" />
        <accelerator
        	id = "window.activeEditor"
        	key = "F12" />
        <accelerator
        	id = "window.nextEditor"
        	key = "Ctrl+F6" />
        <accelerator
        	id = "window.previousEditor"
        	key = "Ctrl+Shift+F6" />
        <accelerator
        	id = "window.nextView"
        	key = "Ctrl+F7" />
        <accelerator
        	id = "window.previousView"
        	key = "Ctrl+Shift+F7" />
    </acceleratorSet>     
</extension>

<extension 
	point="org.eclipse.ui.perspectives">
	<perspective
		id="org.eclipse.ui.resourcePerspective"
		name="%Perspective.resourcePerspective"
		class="org.eclipse.ui.internal.ResourcePerspective"
		icon="icons/full/cview16/resource_persp.gif">
	</perspective>
</extension>

<extension 
	point="org.eclipse.ui.importWizards">
	<wizard 
	   id="org.eclipse.ui.wizards.import.FileSystem"
	   name="%ImportWizards.FileSystem"
	   class="org.eclipse.ui.wizards.datatransfer.FileSystemImportWizard"
	   icon="icons/full/ctool16/importdir_wiz.gif">
	   <description>%ImportWizards.FileSystemDescription</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
	<wizard
	   id="org.eclipse.ui.wizards.import.ZipFile"
	   name="%ImportWizards.ZipFile"
	   class="org.eclipse.ui.wizards.datatransfer.ZipFileImportWizard"
	   icon="icons/full/ctool16/importzip_wiz.gif">
	   <description>%ImportWizards.ZipFileDescription</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
      	</wizard>
</extension>

<extension 
	point="org.eclipse.ui.exportWizards">
	<wizard
	   id="org.eclipse.ui.wizards.export.FileSystem"
	   name="%ExportWizards.FileSystem"
	   class="org.eclipse.ui.wizards.datatransfer.FileSystemExportWizard"
	   icon="icons/full/ctool16/exportdir_wiz.gif">
	   <description>%ExportWizards.FileSystemDescription</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
	<wizard
	   id="org.eclipse.ui.wizards.export.ZipFile"
	   name="%ExportWizards.ZipFile"
	   class="org.eclipse.ui.wizards.datatransfer.ZipFileExportWizard"
	   icon="icons/full/ctool16/exportzip_wiz.gif">
	   <description>%ExportWizards.ZipFileDescription</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
</extension>

<extension 
	point="org.eclipse.ui.views">
	<category 
	   id="org.eclipse.ui"
	   name="%Views.Category.Basic">
	</category>
	<view
       id="org.eclipse.ui.views.ResourceNavigator"
	   name="%Views.ResourceNavigator"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.navigator.ResourceNavigator"
	   icon="icons/full/cview16/filenav_nav.gif">
	</view>
	<view
       id="org.eclipse.ui.views.BookmarkNavigator"
	   name="%Views.BookmarkNavigator"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.bookmarkexplorer.BookmarkNavigator"
	   icon="icons/full/cview16/bkmrk_nav.gif">
	</view>
	<view
       id="org.eclipse.ui.views.PropertySheet"
	   name="%Views.PropertySheet"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.properties.PropertySheet"
	   icon="icons/full/cview16/prop_ps.gif">
	</view>
	<view
	   id="org.eclipse.ui.views.ContentOutline"
	   name="%Views.ContentOutline"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.contentoutline.ContentOutline"
	   icon="icons/full/cview16/outline_co.gif">
	</view>
	<view
	   id="org.eclipse.ui.views.TaskList"
	   name="%Views.TaskList"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.tasklist.TaskList"
	   icon="icons/full/cview16/tasks_tsk.gif">
	</view>
</extension>

<extension 
	point="org.eclipse.ui.markerImageProviders">
	<imageprovider 
		id="org.eclipse.ui.taskMarkerProvider"
		markertype="org.eclipse.core.resources.taskmarker"
		icon="icons/full/cview16/tasks_tsk.gif">
	</imageprovider>
	<imageprovider 
		id="org.eclipse.ui.problemMarkerProvider"
		markertype="org.eclipse.core.resources.problemmarker"
		class="org.eclipse.ui.internal.ProblemImageProvider">
	</imageprovider>
</extension>

<extension 
	point="org.eclipse.ui.newWizards">
	<category
	   id="org.eclipse.ui.Examples"
	   name="%NewWizards.Category.Examples">
	</category>
	<category
	   id="org.eclipse.ui.Basic"
	   name="%NewWizards.Category.Basic">
	</category>
	<wizard 
	   id="org.eclipse.ui.wizards.new.project"
	   name="%NewWizards.project"
	   class="org.eclipse.ui.wizards.newresource.BasicNewProjectResourceWizard"
	   category="org.eclipse.ui.Basic"
	   project="true"
	   finalPerspective="org.eclipse.ui.resourcePerspective"
	   icon="icons/full/ctool16/newprj_wiz.gif">
       <description>%NewWizards.project.description</description>
    </wizard>
	<wizard
	   id="org.eclipse.ui.wizards.new.folder"
	   name="%NewWizards.folder"
	   class="org.eclipse.ui.wizards.newresource.BasicNewFolderResourceWizard"
	   category="org.eclipse.ui.Basic"
	   icon="icons/full/ctool16/newfolder_wiz.gif">
	   <description>%NewWizards.folder.description</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
	<wizard
	   id="org.eclipse.ui.wizards.new.file"
	   name="%NewWizards.file"
	   class="org.eclipse.ui.wizards.newresource.BasicNewFileResourceWizard"
	   category="org.eclipse.ui.Basic"
	   icon="icons/full/ctool16/newfile_wiz.gif">
	   <description>%NewWizards.file.description</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
</extension>

<extension 
	point="org.eclipse.ui.preferencePages">
   	<page id="org.eclipse.ui.preferencePages.Workbench"
          name="%PreferencePages.Workbench"
          class="org.eclipse.ui.internal.dialogs.WorkbenchPreferencePage">
   	</page>
   	<page id="org.eclipse.ui.preferencePages.FileEditors"
   	      name="%PreferencePages.FileEditors"
	      class="org.eclipse.ui.internal.dialogs.FileEditorsPreferencePage"
	      category="org.eclipse.ui.preferencePages.Workbench">
   	</page>
	<page id="org.eclipse.ui.preferencePages.FileStates"
	      name="%PreferencePages.FileStates"
	      class="org.eclipse.ui.internal.dialogs.FileStatesPage"            
	      category="org.eclipse.ui.preferencePages.Workbench">
       </page>
	<page id="org.eclipse.ui.preferencePages.Perspectives"
	      name="%PreferencePages.Perspectives"
	      class="org.eclipse.ui.internal.dialogs.PerspectivesPreferencePage"
          category="org.eclipse.ui.preferencePages.Workbench">
       </page>
	<page id="org.eclipse.ui.preferencePages.Views"
	      name="%PreferencePages.Views"
