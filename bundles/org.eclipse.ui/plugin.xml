       	  category="org.eclipse.ui.preferencePages.Workbench">
       </page>
	 <page id="org.eclipse.ui.preferencePages.Fonts"
	      name="%PreferencePages.Fonts"
	      class="org.eclipse.ui.internal.dialogs.FontPreferencePage"
       	  category="org.eclipse.ui.preferencePages.Workbench">
       </page>
</extension>

<extension 
	point="org.eclipse.ui.preferencePages">
   	<page id="org.eclipse.ui.preferencePages.BuildOrder"
              name="%PreferencePages.BuildOrder"
              class="org.eclipse.ui.internal.dialogs.BuildOrderPreferencePage">
   	</page>
</extension>

<extension
	point="org.eclipse.ui.propertyPages">
   	<page id="org.eclipse.ui.propertypages.project.capability"
   	      name="%PropertyPages.project.capability"
          objectClass="org.eclipse.core.resources.IProject"
          class="org.eclipse.ui.internal.dialogs.ProjectCapabilityPage"
          adaptable="true">
   	</page>
   	<page id="org.eclipse.ui.propertypages.project.reference"
   	      name="%PropertyPages.project.reference"
          objectClass="org.eclipse.core.resources.IProject"
          class="org.eclipse.ui.internal.dialogs.ProjectReferencePage"
          adaptable="true">
   	</page>
   	<page id="org.eclipse.ui.propertypages.info.file"
   	      name="%PropertyPages.info.file"
          objectClass="org.eclipse.core.resources.IResource"
          class="org.eclipse.ui.internal.dialogs.ResourceInfoPage"
          adaptable="true">
   	</page>
</extension>

<extension
    point = "org.eclipse.ui.editors">
    <editor id ="org.eclipse.ui.DefaultTextEditor"
      name="%Editors.DefaultTextEditor"
      icon="icons/full/obj16/file_obj.gif"
      extensions="txt"
      class="org.eclipse.ui.editors.text.TextEditor"
      contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor">     
    </editor>

    <editor id="org.eclipse.ui.internal.dialogs.WelcomeEditor"
      name="%Editors.WelcomeEditor"
      icon="icons/full/obj16/welcome_editor.gif"
      class="org.eclipse.ui.internal.dialogs.WelcomeEditor">
    </editor>
</extension>

<extension
    point = "org.eclipse.ui.elementFactories">
    <factory id ="org.eclipse.ui.internal.model.ResourceFactory"
      class="org.eclipse.ui.internal.model.ResourceFactory">
	</factory>
    <factory id ="org.eclipse.ui.internal.model.WorkspaceFactory"
      class="org.eclipse.ui.internal.model.WorkspaceFactory">
	</factory>
    <factory id ="org.eclipse.ui.part.FileEditorInputFactory"
      class="org.eclipse.ui.part.FileEditorInputFactory">
	</factory>
    <factory id ="org.eclipse.ui.internal.dialogs.WelcomeEditorInputFactory"
      class="org.eclipse.ui.internal.dialogs.WelcomeEditorInputFactory">
	</factory>
</extension>

<extension-point name="%ExtPoint.markerUpdaters" id="markerUpdaters"/>
<extension-point name="%ExtPoint.documentProviders" id="documentProviders"/>

<extension
	point= "org.eclipse.ui.markerUpdaters">
	<updater id="org.eclipse.ui.texteditor.BasicMarkerUpdater"
		class="org.eclipse.ui.texteditor.BasicMarkerUpdater">
	</updater>
</extension>


<extension
	point= "org.eclipse.ui.documentProviders">
	<provider id= "org.eclipse.ui.editors.text.FileDocumentProvider"
		class="org.eclipse.ui.editors.text.FileDocumentProvider"
		inputTypes="org.eclipse.ui.IStorageEditorInput">
	</provider>
</extension>

<extension
	point= "org.eclipse.ui.resourceFilters">
	<filter pattern = ".*" selected = "false"/>
</extension>

</plugin>
