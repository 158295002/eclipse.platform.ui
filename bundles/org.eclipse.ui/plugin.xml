<?xml version="1.0" encoding="UTF-8"?>

<plugin
   name="%Plugin.name"
   id="org.eclipse.ui"
   version="0.9"
   provider-name="Object Technology International, Inc."
   class="org.eclipse.ui.internal.WorkbenchPlugin">
   
   <requires>
      <import plugin="org.apache.xerces"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.swt" export="true" /> 
   </requires>

<runtime>
  <library name="workbench.jar" >
     <export name = "*"/>
  </library> 
</runtime>

<extension id="workbench" point="org.eclipse.core.runtime.applications">
  <application> 
     <run class="org.eclipse.ui.internal.Workbench">
	<parameter name="productInfo" value="product.ini"/>
     </run>
  </application>
</extension>

<!-- Workbench extension point definitions -->

<extension-point name="%ExtPoint.perspectives" id="perspectives"/>
<extension-point name="%ExtPoint.PerspectiveExtensions" id="perspectiveExtensions"/>
<extension-point name="%ExtPoint.PreferencePages" id="preferencePages"/>
<extension-point name="%ExtPoint.PropertyPages" id="propertyPages"/>
<extension-point name="%ExtPoint.Editors" id="editors"/>
<extension-point name="%ExtPoint.Views" id="views"/>
<extension-point name="%ExtPoint.ActionSets" id="actionSets"/>
<extension-point name="%ExtPoint.ViewMenus" id="viewActions"/>
<extension-point name="%ExtPoint.EditorMenus " id="editorActions"/>
<extension-point name="%ExtPoint.PopupMenus " id="popupMenus"/>
<extension-point name="%ExtPoint.ImportWizards" id="importWizards"/>
<extension-point name="%ExtPoint.ExportWizards" id="exportWizards"/>
<extension-point name="%ExtPoint.ResourceCreationWizards" id="newWizards"/>
<extension-point name="%ExtPoint.ElementFactories" id="elementFactories"/>
<extension-point name="%ExtPoint.DropActions" id="dropActions"/>
<extension-point name="%ExtPoint.ResourceFilters" id="resourceFilters"/>
<extension-point name="%ExtPoint.MarkerImage" id="markerImageProviders"/>
<extension-point name="%ExtPoint.ProjectImages " id="projectNatureImages"/>

<!-- Extensions -->

<extension 
	point="org.eclipse.ui.perspectives">
	<perspective
		id="org.eclipse.ui.resourcePerspective"
		name="%Perspective.resource"
		class="org.eclipse.ui.internal.ResourcePerspective">
	</perspective>
</extension>

<extension 
	point="org.eclipse.ui.importWizards">
	<wizard 
	   id="org.eclipse.ui.wizards.import.FileSystem"
	   name="%ImportWizards.fileSystem"
	   class="org.eclipse.ui.wizards.datatransfer.FileSystemImportWizard"
	   icon="icons/basic/ctool16/importdir_wiz.gif">
	   <description>Import resources from the local file system</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
	<wizard
	   id="org.eclipse.ui.wizards.import.ZipFile"
	   name="%ImportWizards.zipFile"
	   class="org.eclipse.ui.wizards.datatransfer.ZipFileImportWizard"
	   icon="icons/basic/ctool16/importzip_wiz.gif">
	   <description>Import resources from a Zip or Jar file on the local file system</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
      	</wizard>
</extension>

<extension 
	point="org.eclipse.ui.exportWizards">
	<wizard
	   id="org.eclipse.ui.wizards.export.FileSystem"
	   name="%ExportWizards.fileSystem"
	   class="org.eclipse.ui.wizards.datatransfer.FileSystemExportWizard"
	   icon="icons/basic/ctool16/exportdir_wiz.gif">
	   <description>Export resources to the local file system</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
	<wizard
	   id="org.eclipse.ui.wizards.export.ZipFile"
	   name="%ExportWizards.zipFile"
	   class="org.eclipse.ui.wizards.datatransfer.ZipFileExportWizard"
	   icon="icons/basic/ctool16/exportzip_wiz.gif">
	   <description>Export resources to a Zip file on the local file system</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
</extension>

<extension 
	point="org.eclipse.ui.views">
	<category 
	   id="org.eclipse.ui"
	   name="%Views.Category.basic">
	</category>
	<view
       id="org.eclipse.ui.views.ResourceNavigator"
	   name="%Views.navigator"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.navigator.ResourceNavigator"
	   icon="icons/basic/cview16/filenav_nav.gif">
	</view>
	<view
       id="org.eclipse.ui.views.BookmarkNavigator"
	   name="%Views.bookmarks"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.bookmarkexplorer.BookmarkNavigator"
	   icon="icons/basic/cview16/bkmrk_nav.gif">
	</view>
	<view
       id="org.eclipse.ui.views.PropertySheet"
	   name="%Views.properties"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.properties.PropertySheet"
	   icon="icons/basic/cview16/prop_ps.gif">
	</view>
	<view
	   id="org.eclipse.ui.views.ContentOutline"
	   name="%Views.outline"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.contentoutline.ContentOutline"
	   icon="icons/basic/cview16/outline_co.gif">
	</view>
	<view
	   id="org.eclipse.ui.views.TaskList"
	   name="%Views.tasks"
	   category="org.eclipse.ui"
	   class="org.eclipse.ui.views.tasklist.TaskList"
	   icon="icons/basic/cview16/tasks_tsk.gif">
	</view>
</extension>

<extension 
	point="org.eclipse.ui.markerImageProviders">
	<imageprovider 
		id="org.eclipse.ui.taskMarkerProvider"
		markertype="org.eclipse.core.resources.taskmarker"
		icon="icons/basic/cview16/tasks_tsk.gif">
	</imageprovider>
	<imageprovider 
		id="org.eclipse.ui.problemMarkerProvider"
		markertype="org.eclipse.core.resources.problemmarker"
		class="org.eclipse.ui.internal.ProblemImageProvider">
	</imageprovider>
</extension>

<extension 
	point="org.eclipse.ui.newWizards">
	<category
	   id="org.eclipse.ui.Basic"
	   name="%NewWizards.Category.simple">
	</category>
	<wizard 
	   id="org.eclipse.ui.wizards.new.project"
	   name="%NewWizards.project"
	   class="org.eclipse.ui.wizards.newresource.BasicNewProjectResourceWizard"
	   category="org.eclipse.ui.Basic"
	   project="true"
	   finalPerspective="org.eclipse.ui.resourcePerspective"
	   icon="icons/basic/ctool16/newprj_wiz.gif">
           <description>%NewWizards.project.description</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
        </wizard>
	<wizard
	   id="org.eclipse.ui.wizards.new.folder"
	   name="%NewWizards.folder"
	   class="org.eclipse.ui.wizards.newresource.BasicNewFolderResourceWizard"
	   category="org.eclipse.ui.Basic"
	   icon="icons/basic/ctool16/newfolder_wiz.gif">
	   <description>%NewWizards.folder.description</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
	<wizard
	   id="org.eclipse.ui.wizards.new.file"
	   name="%NewWizards.file"
	   class="org.eclipse.ui.wizards.newresource.BasicNewFileResourceWizard"
	   category="org.eclipse.ui.Basic"
	   icon="icons/basic/ctool16/newfile_wiz.gif">
	   <description>%NewWizards.file.description</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
</extension>

<extension 
	point="org.eclipse.ui.preferencePages">
   	<page id="org.eclipse.ui.preferencePages.Workbench"
              name="%PreferencePages.workbench"
              class="org.eclipse.ui.internal.dialogs.WorkbenchPreferencePage">
   	</page>
   	<page name="%PreferencePages.fileEditors"
              id="org.eclipse.ui.preferencePages.FileEditors"
	      class="org.eclipse.ui.internal.dialogs.FileEditorsPreferencePage"
	      category="org.eclipse.ui.preferencePages.Workbench">
   	</page>
	<page id="org.eclipse.ui.preferencePages.FileStates"
	      class="org.eclipse.ui.internal.dialogs.FileStatesPage"
            name="%PreferencePages.localHistory"
		category="org.eclipse.ui.preferencePages.Workbench">
       </page>
	<page id="org.eclipse.ui.preferencePages.Perspectives"
	      class="org.eclipse.ui.internal.dialogs.PerspectivesPreferencePage"
            name="%PreferencePages.perspectives"
		category="org.eclipse.ui.preferencePages.Workbench">
       </page>
	<page id="org.eclipse.ui.preferencePages.Views"
	      class="org.eclipse.ui.internal.dialogs.ViewsPreferencePage"
            name="%PreferencePages.appearance"
		category="org.eclipse.ui.preferencePages.Workbench">
       </page>

</extension>

<extension 
	point="org.eclipse.ui.preferencePages">
   	<page id="org.eclipse.ui.preferencePages.BuildOrder"
              name="%PreferencePages.buildOrder"
              class="org.eclipse.ui.internal.dialogs.BuildOrderPreferencePage">
   	</page>
</extension>

<extension
	point="org.eclipse.ui.propertyPages">
   	<page name="%PropertyPages.projectReferences"
  	   id="org.eclipse.ui.propertypages.project.reference"
           objectClass="org.eclipse.core.resources.IProject"
           class="org.eclipse.ui.internal.dialogs.ProjectReferencePage">
   	</page>
   	<page name="%PropertyPages.info"
  	   id="org.eclipse.ui.propertypages.info.file"
           objectClass="org.eclipse.core.resources.IResource"
           class="org.eclipse.ui.internal.dialogs.ResourceInfoPage">
   	</page>
</extension>

<extension
    point = "org.eclipse.ui.editors">
    <editor id ="org.eclipse.ui.DefaultTextEditor"
      icon="icons/basic/obj16/file_obj.gif"
      name="%Editors.defaultTextEditor"
      extensions="txt"
      class="org.eclipse.ui.editors.text.TextEditor"
      contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor">     
    </editor>

    <editor id="org.eclipse.ui.internal.dialogs.WelcomeEditor"
      name="%Editors.welcome"
      icon="icons/basic/obj16/welcome_editor.gif"
      class="org.eclipse.ui.internal.dialogs.WelcomeEditor">
    </editor>
</extension>

<extension
    point = "org.eclipse.ui.elementFactories">
    <factory id ="org.eclipse.ui.internal.model.ResourceFactory"
      class="org.eclipse.ui.internal.model.ResourceFactory">
	</factory>
    <factory id ="org.eclipse.ui.internal.model.WorkspaceFactory"
      class="org.eclipse.ui.internal.model.WorkspaceFactory">
	</factory>
    <factory id ="org.eclipse.ui.part.FileEditorInputFactory"
      class="org.eclipse.ui.part.FileEditorInputFactory">
	</factory>
    <factory id ="org.eclipse.ui.internal.dialogs.WelcomeEditorInputFactory"
      class="org.eclipse.ui.internal.dialogs.WelcomeEditorInputFactory">
	</factory>
</extension>

<extension-point name="%ExtPoint.markerUpdaters" id="markerUpdaters"/>
<extension-point name="%ExtPoint.documentProviders" id="documentProviders"/>

<extension
	point= "org.eclipse.ui.markerUpdaters">
	<updater id="org.eclipse.ui.texteditor.BasicMarkerUpdater"
		class="org.eclipse.ui.texteditor.BasicMarkerUpdater">
	</updater>
</extension>


<extension
	point= "org.eclipse.ui.documentProviders">
	<provider id= "org.eclipse.ui.editors.text.FileDocumentProvider"
		class="org.eclipse.ui.editors.text.FileDocumentProvider"
		inputTypes="org.eclipse.ui.IStorageEditorInput">
	</provider>
</extension>

<extension
	point= "org.eclipse.ui.resourceFilters">
	<filter pattern = ".*" selected = "false"/>
</extension>

<extension 
	point="org.eclipse.ui.preferencePages">
	<page id="org.eclipse.ui.preferencePages.TextEditor"
	      class="org.eclipse.ui.editors.text.TextEditorPreferencePage"
          name="%PreferencePages.defaultTextEditor"
	      category="org.eclipse.ui.preferencePages.Workbench">
    	</page>
</extension>

</plugin>
